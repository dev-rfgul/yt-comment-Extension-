{
    "manifest_version": 3,
    "name": "Youtube Auto Interact  ",
    "version": "1.0",
    "description": "It will play the video, like it and comment on it using groq ai.",
    "permissions": [
      "activeTab",
      "tabs",
      "storage",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "content.js"
        ]
      }
    ] 
  }